{
  "name": "express-es6-api",
  "version": "0.1.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec babel-node --inspect src",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "eslint": "eslint",
    "lint": "npm run eslint -- ./",
    "test": "mocha --compilers js:babel-register --recursive test/"
  },
  "repository": "https://gitlab.com/viseo-digital-squads/express-es6-api",
  "author": "Viseo Digital Squad",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "bluebird": "^3.4.7",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-jwt": "^5.1.0",
    "express-unless": "^0.3.0",
    "express-winston": "^2.1.3",
    "jsonwebtoken": "^7.2.1",
    "lodash": "^4.17.4",
    "mime": "^1.3.6",
    "mongodb": "2.1.17",
    "multer": "^1.3.0",
    "node-linkedin": "^0.5.4",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "request-promise": "^4.1.1",
    "sails-mongo": "^0.12.2",
    "waterline": "^0.11.6",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "nodemon": "^1.9.2",
    "should": "^11.1.2",
    "supertest": "^2.0.1"
  }
}
